{
  "name": "@guardian/cdk-cli",
  "description": "A CLI tool to migrate from cloudformation to Guardian flavoured CDK",
  "version": "1.2.0",
  "author": "The Guardian",
  "bin": {
    "cdk-cli": "./bin/run"
  },
  "bugs": "https://github.com/guardian/cdk-cli/issues",
  "dependencies": {
    "@aws-cdk/core": "1.132.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3.2.4",
    "codemaker": "^1.44.0",
    "js-yaml": "^4.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.upperfirst": "^4.3.1",
    "prompts": "^2.4.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^0.7.0",
    "@oclif/dev-cli": "^1",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.4",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "16.11.7",
    "@types/prompts": "^2.0.14",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/guardian/cdk-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cdk-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "guardian/cdk-cli",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && yarn copy-files && oclif-dev readme",
    "copy-files": "copyfiles -a -u 1 \"./src/template/**/*\" lib",
    "build": "yarn prepack",
    "test": "jest --detectOpenHandles --runInBand",
    "lint": "eslint src/** --ext .ts --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
