{
  "name": "@guardian/cdk-cli",
  "description": "A CLI tool to migrate from cloudformation to Guardian flavoured CDK",
  "version": "0.0.0",
  "author": "The Guardian",
  "bin": {
    "cdk-cli": "./bin/run"
  },
  "bugs": "https://github.com/guardian/cdk-cli/issues",
  "dependencies": {
    "@aws-cdk/core": "^1.86.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "codemaker": "^1.17.1",
    "js-yaml": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.upperfirst": "^4.3.1",
    "prompts": "^2.4.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^0.4.2",
    "@oclif/dev-cli": "^1",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/node": "14.14.22",
    "@types/prompts": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9",
    "typescript": "~4.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/guardian/cdk-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cdk-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "guardian/cdk-cli",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && yarn copy-files && oclif-dev readme",
    "copy-files": "copyfiles -a -u 1 \"./src/template/**/*\" lib",
    "build": "yarn prepack",
    "test": "jest --detectOpenHandles --runInBand",
    "lint": "eslint src/** --ext .ts --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
